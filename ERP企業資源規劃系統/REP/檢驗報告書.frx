    Private Sub Data2_BeforePrint(ByVal sender As object, ByVal e As EventArgs) 
      Data2.Visible=true
      Report.SetParameterValue(&quot;VName&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V0&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V1&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V2&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V3&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V4&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V5&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V6&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V7&quot;, &quot;&quot;)
      Report.SetParameterValue(&quot;V8&quot;, &quot;&quot;)
      dim s1 as string= CType(Report.GetColumnValue(&quot;明細.檢驗項目&quot;), String)
      if s1=&quot;斷面積&quot; then
        if CType(Report.GetColumnValue(&quot;主檔.CK斷面積值起迄&quot;), String)&lt;&gt;&quot;True&quot; then
            Data2.Visible=false
        end if
        Report.SetParameterValue(&quot;VName&quot;, &quot;斷面積 (mm2)&quot;+vbcrlf+&quot;Effect Cross&quot;+vbcrlf+&quot;Calibration&quot;) 
        Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.斷面積&quot;), String))
      end if
      
      if s1=&quot;最大負荷&quot; then
        if CType(Report.GetColumnValue(&quot;主檔.CK最大負荷值起迄&quot;), String)&lt;&gt;&quot;True&quot; then
          Data2.Visible=false
        end if        
        Report.SetParameterValue(&quot;VName&quot;, &quot;最大負荷 Kg&quot;+vbcrlf+&quot;Max Load&quot;) 
        Report.SetParameterValue(&quot;V0&quot;, &quot;&quot;)
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值1&quot;), Double))
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值2&quot;), Double))
        Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值3&quot;), Double))
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值4&quot;), Double))
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值5&quot;), Double))
        Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值6&quot;), Double))
        Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值7&quot;), Double))
        Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.最大負荷值8&quot;), Double))
       
      end if 
      
      if s1=&quot;抗拉強度&quot; then
        if not (CType(Report.GetColumnValue(&quot;主檔.CK抗拉強度&quot;), String)=&quot;True&quot; or CType(Report.GetColumnValue(&quot;主檔.CK抗拉強度值&quot;), String)=&quot;True&quot;)  then
          Data2.Visible=false
        end if                  
        Report.SetParameterValue(&quot;VName&quot;, &quot;抗拉強度 (kg/mm2)&quot;+vbcrlf+&quot;Tensile Strength&quot;) 
        if CType(Report.GetColumnValue(&quot;主檔.CK抗拉強度&quot;), String)=&quot;True&quot; then
          Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度&quot;), String))
        end if
        if CType(Report.GetColumnValue(&quot;主檔.CK抗拉強度值&quot;), String)=&quot;True&quot; then
          Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值&quot;), String))
        end if
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值1&quot;), Double))
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值2&quot;), Double))
        Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值3&quot;), Double))
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值4&quot;), Double))
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值5&quot;), Double))
        Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值6&quot;), Double))
        Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值7&quot;), Double))
        Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值8&quot;), Double))
      end if
      
      if s1=&quot;降伏點&quot; then
        if CType(Report.GetColumnValue(&quot;主檔.CK降伏點值起迄&quot;), String)&lt;&gt;&quot;True&quot; then
          Data2.Visible=false
        end if 
        
        Report.SetParameterValue(&quot;VName&quot;, &quot;降伏點 (kg)&quot;+vbcrlf+&quot;Yield Point&quot;) 
        Report.SetParameterValue(&quot;V0&quot;, &quot;&quot;)
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值1&quot;), Double))
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值2&quot;), Double))
        Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值3&quot;), Double))
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值4&quot;), Double))
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值5&quot;), Double))
        Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值6&quot;), Double))
        Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值7&quot;), Double))
        Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏點值8&quot;), Double))
      end if
                  
      if s1=&quot;降伏點強度&quot; then
        if CType(Report.GetColumnValue(&quot;主檔.CK降伏強度值&quot;), String)&lt;&gt;&quot;True&quot; then
          Data2.Visible=false
        end if 
        
        Report.SetParameterValue(&quot;VName&quot;, &quot;降伏點強度 (kg/Mm2)&quot;+vbcrlf+&quot;Yield Strength&quot;) 
        Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值&quot;), String))
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值1&quot;), Double))
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值2&quot;), Double))
        Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值3&quot;), Double))
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值4&quot;), Double))
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值5&quot;), Double))
        Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值6&quot;), Double))
        Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值7&quot;), Double))
        Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.降伏強度值8&quot;), Double))
      end if
      
      if s1=&quot;伸長率&quot; then
        if CType(Report.GetColumnValue(&quot;主檔.CK伸長率值&quot;), String)&lt;&gt;&quot;True&quot; then
          Data2.Visible=false
        end if 
        
        Report.SetParameterValue(&quot;VName&quot;, &quot;伸長率 (%)&quot;+vbcrlf+&quot;Elongation&quot;) 
        Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值&quot;), String))
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值1&quot;), Double))
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值2&quot;), Double))
        Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值3&quot;), Double))
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值4&quot;), Double))
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值5&quot;), Double))
        Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值6&quot;), Double))
        Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值7&quot;), Double))
        Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.伸長率值8&quot;), Double))
      end if
      
      if s1=&quot;表面硬度&quot; then
        if not ( CType(Report.GetColumnValue(&quot;主檔.CK表面硬度&quot;), String)=&quot;True&quot; or CType(Report.GetColumnValue(&quot;主檔.CK表面硬度值&quot;), String)=&quot;True&quot;) then
          Data2.Visible=false
        end if 
        
        Report.SetParameterValue(&quot;VName&quot;, &quot;表面硬度(&quot;+CType(Report.GetColumnValue(&quot;主檔.表面硬度規格&quot;), String)+&quot;)&quot;+vbcrlf+&quot;Surface Hardness&quot;) 
        if CType(Report.GetColumnValue(&quot;主檔.CK表面硬度&quot;), String)=&quot;True&quot; then
          Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度&quot;), String))
        end if
        if CType(Report.GetColumnValue(&quot;主檔.CK表面硬度值&quot;), String)=&quot;True&quot; then
          Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值&quot;), String))
        end if
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值1&quot;), Double))
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值2&quot;), Double))
        Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值3&quot;), Double))
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值4&quot;), Double))
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值5&quot;), Double))
        Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值6&quot;), Double))
        Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值7&quot;), Double))
        Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值8&quot;), Double))
      end if   
      
      if s1=&quot;心部硬度&quot; then
        if not ( CType(Report.GetColumnValue(&quot;主檔.CK心部硬度&quot;), String)=&quot;True&quot; or CType(Report.GetColumnValue(&quot;主檔.CK心部硬度值&quot;), String)=&quot;True&quot;) then
          Data2.Visible=false
        end if 
        
        Report.SetParameterValue(&quot;VName&quot;, &quot;心部硬度(&quot;+CType(Report.GetColumnValue(&quot;主檔.心部硬度規格&quot;), String)+&quot;)&quot;+vbcrlf+&quot;Core Hardness&quot;) 
        if CType(Report.GetColumnValue(&quot;主檔.CK心部硬度&quot;), String)=&quot;True&quot; then
          Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度&quot;), String))
        end if
        if CType(Report.GetColumnValue(&quot;主檔.CK心部硬度值&quot;), String)=&quot;True&quot; then
          Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值&quot;), String))
        end if
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值1&quot;), Double))
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值2&quot;), Double))
        Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值3&quot;), Double))
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值4&quot;), Double))
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值5&quot;), Double))
        Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值6&quot;), Double))
        Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值7&quot;), Double))
        Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值8&quot;), Double))
      end if
             
      if s1=&quot;滲碳層&quot; then
        if not ( CType(Report.GetColumnValue(&quot;主檔.CK滲碳層&quot;), String)=&quot;True&quot; or CType(Report.GetColumnValue(&quot;主檔.CK滲碳層值&quot;), String)=&quot;True&quot;) then
          Data2.Visible=false
        end if 
        
       
        Report.SetParameterValue(&quot;VName&quot;, &quot;滲碳層 (m/m)&quot;+vbcrlf+&quot;Case Depth&quot;) 
         
