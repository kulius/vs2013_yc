        end if
        if CType(Report.GetColumnValue(&quot;主檔.CK滲碳層值&quot;), String)=&quot;True&quot; then
          Report.SetParameterValue(&quot;V0&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層1值&quot;), String))
        end if
        
        If   CType(Report.GetColumnValue(&quot;主檔.CK滲碳層2值&quot;), String)=&quot;True&quot; Then
          Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值1&quot;), Double))
          Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值2&quot;), Double))
          Report.SetParameterValue(&quot;V3&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值3&quot;), Double))
          Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值4&quot;), Double))
          Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值5&quot;), Double))
          Report.SetParameterValue(&quot;V6&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值6&quot;), Double))
          Report.SetParameterValue(&quot;V7&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值7&quot;), Double))
          Report.SetParameterValue(&quot;V8&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層2值8&quot;), Double))
        end if
        
        If CType(Report.GetColumnValue(&quot;主檔.CK滲碳層1值&quot;), String)=&quot;True&quot; Then
          if CType(Report.GetColumnValue(&quot;主檔.滲碳層1值1&quot;), String)&lt;&gt;&quot;0&quot; then
