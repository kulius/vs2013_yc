    Private Sub ReportTitle1_BeforePrint(ByVal sender As object, ByVal e As EventArgs)
      if CType(Report.GetColumnValue(&quot;主檔.CK表面硬度&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度&quot;), String))
      end if
      if CType(Report.GetColumnValue(&quot;主檔.CK表面硬度值&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V1&quot;, CType(Report.GetColumnValue(&quot;主檔.表面硬度值&quot;), String))
      end if
      
      if CType(Report.GetColumnValue(&quot;主檔.CK心部硬度&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度&quot;), String))
      end if
      if CType(Report.GetColumnValue(&quot;主檔.CK心部硬度值&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V2&quot;, CType(Report.GetColumnValue(&quot;主檔.心部硬度值&quot;), String))
      end if
      
      if CType(Report.GetColumnValue(&quot;主檔.CK抗拉強度&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度&quot;), String))
      end if
      if CType(Report.GetColumnValue(&quot;主檔.CK抗拉強度值&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V4&quot;, CType(Report.GetColumnValue(&quot;主檔.抗拉強度值&quot;), String))
      end if
      
      if CType(Report.GetColumnValue(&quot;主檔.CK滲碳層&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層&quot;), String))
      end if
      if CType(Report.GetColumnValue(&quot;主檔.CK滲碳層1值&quot;), String)=&quot;True&quot; then
        Report.SetParameterValue(&quot;V5&quot;, CType(Report.GetColumnValue(&quot;主檔.滲碳層1值&quot;), String))
      end if
    End Sub


